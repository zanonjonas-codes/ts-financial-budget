generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model OauthProvider {
  id           Int    @id @default(autoincrement())
  providerName String
  User         User[]
}

model User {
  id             Int            @id @default(autoincrement())
  firstName      String
  lastName       String
  email          String
  password       String
  oathProviderId Int?
  OauthProvider  OauthProvider? @relation(fields: [oathProviderId], references: [id], onDelete: NoAction, onUpdate: NoAction)
}
